{
  "manifest_version": 2,
  "name": "ExCite",
  "version": "1.0",
  "description": "Combs your history and let's you cite",
  "content_scripts": 
  [
    {
      "matches": ["*://*/*"],
      "js": ["js/app.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "book.png",
    "default_popup": "index.html"
  },
  "permissions":[
    "history"
  ]
}