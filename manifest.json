{
  "manifest_version": 2,
  "name": "ExCite",
  "version": "1.0",
  "description": "Combs your history and let's you cite",

  "browser_action": {
    "default_icon": "book.png",
    "default_popup": "index.html"
  },
  "content_scripts": [ {
    "js": ["js/app.js", "jquery.min.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"],
    "run_at": "document_end"
  }],
  "permissions":[
    "history"
  ]
}